{
  "name": "@arbiter/database",
  "version": "0.1.0",
  "description": "SQLite database layer for Arbiter with comprehensive run logging",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "sqlite3": "^5.1.6",
    "@arbiter/core": "workspace:*"
  },
  "devDependencies": {
    "@types/sqlite3": "^3.1.11",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/**/*.test.ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}